# Arca Bank Environment Configuration
# Copy this file to .env and fill in your values

# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token_here

# Database URL (default: SQLite)
# For SQLite (local development):
ARCA_DATABASE_URL=sqlite:///arca_bank.db
# For PostgreSQL (production):
# ARCA_DATABASE_URL=postgresql://user:password@localhost:5432/arca_bank

# Debug Mode
ARCA_DEBUG=false

# Discord Role IDs (optional - for role-based permissions)
# HEAD_BANKER_ROLE_ID=123456789
# BANKER_ROLE_ID=987654321
